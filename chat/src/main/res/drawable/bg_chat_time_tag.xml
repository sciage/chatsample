<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- The background color is set to transparent -->
	<solid android:color="#999"/>
	<!-- Set the black border -->
	<stroke android:width="1px"
	    android:color="#999999"/>
	<!-- Specifies the four rounded corners of the rounded rectangle -->
	<corners 
	    android:topLeftRadius="4px"
	    android:topRightRadius="4px"
	    android:bottomLeftRadius="4px"
	    android:bottomRightRadius="4px"/>
</shape>